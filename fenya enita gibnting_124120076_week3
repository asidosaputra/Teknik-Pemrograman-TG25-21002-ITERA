def c_to_f(c):
    "Konversi Celcius ke Fahrenheit"
    return (c * 9/5) + 32

def f_to_c(f):
    "Konversi Fahrenheit ke Celcius"
    return (f - 32) * 5/9

c = float(input("Masukkan suhu Celcius: "))

f = konversi.c_to_f(c)

print(f"Suhu dalam Fahrenheit = {f}")
