total_belanja = 110000
pake_kupon = True

if total_belanja >= 100000:
  if pake_kupon:
    diskon = 0.2*total_belanja
    print(f"Total Diskon adalah {diskon}, Terimakasih!")
    total_belanja = total_belanja - diskon
    print("Total Belanja : ", total_belanja)
  else:
   print("Total Belanja :", total_belanja)
else:
   print("Belanja kurang dari RP. 100.000")

